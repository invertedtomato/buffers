<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <AssemblyName>InvertedTomato.Buffers</AssemblyName>
    <RootNamespace>InvertedTomato</RootNamespace>
    <Version>1.0.11</Version>
    <PackageLicenseUrl>https://github.com/invertedtomato/buffers/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/invertedtomato/buffers</PackageProjectUrl>
    <RepositoryUrl>https://github.com/invertedtomato/buffers</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>Set of tools for manipulating bits and byte-arrays quickly and efficently. There's nothing earth shattering, though it does saves pages of monkey-work pushing bytes around.</Description>
    <Copyright>Inverted Tomato</Copyright>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageTags>buffers bitoperations</PackageTags>
    <AssemblyVersion>1.0.11.0</AssemblyVersion>
    <FileVersion>1.0.11.0</FileVersion>
    <Authors>Inverted Tomato</Authors>
    <Company>Inverted Tomato</Company>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO" Version="4.3.0" />
    <PackageReference Include="System.Text.RegularExpressions" Version="4.3.0" />
  </ItemGroup>

</Project>